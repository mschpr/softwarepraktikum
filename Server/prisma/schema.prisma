generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model VokabelEnglisch {
  ID           Int    @id @default(autoincrement())
  Vokabel      String @db.VarChar
  Uebersetzung String @db.VarChar
}

model LernfortschrittEnglisch {
  ID        Int       @id @default(autoincrement())
  IDVokabel Int?
  Datum     DateTime? @db.Date
  Stufe     Int?
}
